{
  "name": "vue-video-player",
  "description": "video.js component for Vue",
  "version": "5.0.2",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Surmon",
    "email": "surmon@foxmail.com",
    "url": "https://surmon.me"
  },
  "bugs": {
    "url": "https://github.com/shijinyu/vue-video-player/issues"
  },
  "homepage": "https://github.com/shijinyu/vue-video-player#readme",
  "main": "dist/vue-video-player.js",
  "module": "dist/vue-video-player.esm.js",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "jspm": {
    "main": "dist/vue-video-player.js",
    "registry": "npm",
    "format": "esm"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shijinyu/vue-video-player.git"
  },
  "keywords": [
    "vue-video-player",
    "vue video player",
    "video player",
    "vue player",
    "vue video"
  ],
  "scripts": {
    "build:spa": "cross-env NODE_ENV=production webpack --config config/build.conf.js",
    "build:ssr": "babel src/ssr.js --out-file dist/ssr.js",
    "build": "npm run build:spa && npm run build:ssr",
    "unit": "cross-env BABEL_ENV=test NODE_ENV=testing karma start test/unit/karma.conf.js --watch",
    "test": "cross-env BABEL_ENV=test NODE_ENV=testing karma start test/unit/karma.conf.js --single-run",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "finish": "npm run lint && npm test && npm run build",
    "publish": "git push && git push --tags && npm publish"
  },
  "dependencies": {
    "object-assign": "^4.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "video.js": "^7.3.0",
    "videojs-contrib-hls": "^5.14.1",
    "videojs-flash": "^2.1.2",
    "videojs-hotkeys": "^0.2.22"
  },
  "expDependencies": {
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "vue-video-player": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^0.1.0",
    "babel-eslint": "^10.0.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "eventsource-polyfill": "^0.9.6",
    "extend": "^3.0.2",
    "karma": "^3.1.1",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "lolex": "^1.5.2",
    "mocha": "^3.2.0",
    "opn": "^4.0.2",
    "ora": "^0.3.0",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss": "^7.0.5",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-vue": "^4.3.2",
    "semver": "^5.3.0",
    "shelljs": "^0.7.4",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "vue-eslint-parser": "^3.2.2",
    "vue-template-compiler": "^2.5.17"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
